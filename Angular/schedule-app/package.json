    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "npm run build:dev",
        "build:ng": "npm run rimraf:dist && ng build --watch",
        "build:dev": "npm run rimraf:dist && npm run webpack:dev --watch",
        "build:prod": "npm run rimraf:prod && npm run webpack:prod",
        "rimraf:dist": "node_modules/.bin/rimraf dist",
        "rimraf:prod": "node_modules/.bin/rimraf ../../sberlifelk",
        "webpack:dev": "node node_modules/webpack/bin/webpack.js --config webpack.dev.js --progress --profile",
        "webpack:prod": "node node_modules/webpack/bin/webpack.js --config webpack.prod.js --profile --bail",
        "postinstall": "npm rebuild node-sass",
        "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit"
    },
    "private": true,
